{
    "compilerOptions": {
        "declaration": false,
        "esModuleInterop": true,
        "lib": ["esnext"],
        "module": "commonjs",
        "moduleResolution": "node",
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitReturns": true,
        "noImplicitAny": false,
        "outDir": "dist",
        "skipLibCheck": true,
        "types": ["node"],
        "strict": true,
        "target": "esnext"
    }
}
